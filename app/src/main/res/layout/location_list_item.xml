<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools">
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:gravity="center_vertical"
        android:paddingHorizontal="10dp"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/pummokcode"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="10dp"
            tools:text="G22042600391"
            android:textSize="12dp"
            android:textColor="@color/color_000000"/>

        <TextView
            android:id="@+id/pummyeong"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="10dp"
            tools:text="G22042600391"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/dobeon_model"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="10dp"
            tools:text="G22042600391"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/saying"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="10dp"
            tools:text="G22042600391"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/danwi"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="10dp"
            tools:text="5"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/location"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="10dp"
            tools:text="광명창고"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/hyeonjaegosuryang"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="10dp"
            tools:text="3"
            android:textSize="12dp"
            android:textColor="@color/black" />


        <EditText
            android:id="@+id/edtWarehouse"
            android:layout_width="300dp"
            android:layout_height="30dp"
            android:paddingHorizontal="10dp"
            android:background="@drawable/light_gray_box"
            android:maxLines="1"
            android:inputType="text"
            android:selectAllOnFocus="true"
            android:imeOptions="actionNext"
            tools:text="내외산업 WBS 32"
            android:privateImeOptions="defaultInputmode=english"
            android:textSize="12dp"
            android:textColor="@color/black" />


    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/color_E0E0E0"/>

</LinearLayout>
</layout>