<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools">
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:gravity="center_vertical"
        android:paddingHorizontal="10dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/seq"
            android:layout_width="35dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="3dp"
            tools:text="1"
            android:textSize="12dp"
            android:textColor="@color/color_000000"/>

        <TextView
            android:id="@+id/kittingbeonho"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="3dp"
            tools:text="K22042700001"
            android:textSize="12dp"
            android:textColor="@color/color_000000"/>

        <TextView
            android:id="@+id/kittingdeungrokil"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="3dp"
            tools:text="20220402"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/kittingja"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="3dp"
            tools:text="일지매"
            android:textSize="12dp"
            android:textColor="@color/black" />


        <TextView
            android:id="@+id/bulchulchanggo"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="3dp"
            tools:text="자재창고2"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/bigo"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="3dp"
            tools:text="뉴스티치 2대 일반 스티치 3대"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/yocheongbeonho"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="3dp"
            tools:text="R22041800077,R22041800078"
            android:textSize="12dp"
            android:textColor="@color/black" />


        <TextView
            android:id="@+id/chulgojisicode"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="3dp"
            tools:text="C22-A43-00,C22-A55-00"
            android:textSize="12dp"
            android:textColor="@color/black" />


    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/color_E0E0E0"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@color/color_F7F7F7">

        <TextView
            android:id="@+id/deviceinfo"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginStart="30dp"
            android:gravity="center_vertical"
            tools:text="ㄴ중간재 1번 / 품명 / 도번모델 / 사양"
            android:textSize="12dp"
            android:textColor="@color/color_000000"/>


    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/color_E0E0E0"/>
</LinearLayout>
</layout>