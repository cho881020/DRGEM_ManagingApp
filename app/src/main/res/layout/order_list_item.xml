<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:paddingHorizontal="10dp"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/seq"
            android:layout_width="35dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="3dp"
            tools:text="00014"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/georaecheocode"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="3dp"
            tools:text="00014"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/georaecheomyeong"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="3dp"
            tools:text="(주)경일코퍼레이션"
            android:textSize="12dp"
            android:textColor="@color/color_000000" />

        <TextView
            android:id="@+id/baljubeonho"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="3dp"
            tools:text="B22032200004"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/baljuil"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="3dp"
            tools:text="20220322"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/nappumjangso"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="3dp"
            tools:text="null"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/bigo"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="3dp"
            tools:text="Purchase for XRpad2 3025 LBC-2, Battery charger"
            android:textSize="12dp"
            android:textColor="@color/black" />


    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/color_E0E0E0"/>

</LinearLayout>
</layout>