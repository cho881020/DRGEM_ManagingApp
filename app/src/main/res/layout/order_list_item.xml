<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">
<LinearLayout
    android:layout_width="800dp"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center_vertical"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/seq"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="1"
            android:textSize="12dp"
            android:textColor="@color/color_000000" />


        <TextView
            android:id="@+id/georaecheomyeong"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:text="11111"
            android:textSize="12dp"
            android:maxLines="1"
            android:textColor="@color/color_000000" />

        <TextView
            android:id="@+id/baljubeonho"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="00전자"
            android:textSize="12dp"
            android:textColor="@color/black" />



        <TextView
            android:id="@+id/georaecheocode"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="B22042200003"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/baljuil"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="20220511"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/nappumjangso"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="디알젬광명"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/bigo"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:text="연구소 토파즈 정부과제 샘플구매"
            android:textSize="12dp"
            android:maxLines="1"
            android:ellipsize="end"
            android:textColor="@color/black" />


    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/color_E0E0E0"/>

</LinearLayout>
</layout>