<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools">
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/color_FFFFFF"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingHorizontal="1dp">

        <TextView
            android:id="@+id/seq"
            android:layout_width="35dp"
            android:layout_height="wrap_content"
            android:paddingRight="3dp"
            android:gravity="center"
            tools:text="14"
            android:maxLines="1"
            android:ellipsize="end"
            android:textSize="12dp"
            android:textColor="@color/color_000000"/>

        <TextView
            android:id="@+id/pummokcode"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingRight="3dp"
            tools:text="E08-000601-00"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/pummyeong"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:paddingRight="3dp"
            android:maxLines="1"
            android:ellipsize="end"
            tools:text="E08-000601-00"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/dobeon_model"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:paddingRight="3dp"
            android:maxLines="1"
            android:ellipsize="end"
            tools:text="E08-000601-00"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/sayang"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:paddingRight="3dp"
            tools:text="E08-000601-00"
            android:maxLines="1"
            android:ellipsize="end"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/balhudanwi"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:paddingRight="3dp"
            tools:text="0004"
            android:maxLines="1"
            android:ellipsize="end"
            android:textSize="12dp"
            android:textColor="@color/black" />


        <TextView
            android:id="@+id/baljusuryang"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:paddingRight="3dp"
            tools:text="12"
            android:maxLines="1"
            android:ellipsize="end"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/giipgosuryang"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:paddingRight="3dp"
            tools:text="12"
            android:maxLines="1"
            android:ellipsize="end"
            android:textSize="12dp"
            android:textColor="@color/black" />


        <LinearLayout
            android:layout_width="54dp"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/ipgosuryang"
                android:layout_width="50dp"
                android:layout_height="30dp"
                android:maxLines="1"
                android:inputType="number"
                tools:text="1"
                android:selectAllOnFocus="true"
                android:imeOptions="actionNext"
                android:textSize="12dp"
                android:gravity="center"
                android:background="@drawable/light_gray_box"
                android:textColor="@color/black" />

        </LinearLayout>

            <TextView
                android:id="@+id/btnEdit"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:visibility="gone"
                android:gravity="center"
                android:text="정보입력"
                android:textSize="12dp"
                android:background="@drawable/btn_light_gray"
                android:textColor="@color/color_9A9A9A" />

        <TextView
            android:id="@+id/location"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:paddingRight="3dp"
            tools:text="공장위치"
            android:maxLines="1"
            android:ellipsize="end"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/baljubeonho"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:paddingRight="3dp"
            tools:text="B22042200003"
            android:maxLines="1"
            android:ellipsize="end"
            android:textSize="12dp"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/baljuseq"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:paddingRight="3dp"
            tools:text="3"
            android:maxLines="1"
            android:ellipsize="end"
            android:textSize="12dp"
            android:textColor="@color/black" />


    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/color_E0E0E0"/>

</LinearLayout>
</layout>